OAAA,EAAA;AACA,IAAA,KADA,mBACA;AACA;AACA,UAAA,KAAA,KAAA,CAAA,IAAA,CAAA,QAAA,EAAA,EAAA;AACA,aAAA,OAAA,GAAA,IAAA;AACA,aAAA,MAAA,CAAA,QAAA,CAAA,aAAA,EAAA,KAAA,WAAA,EAFA,CAEA;;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,MAAA,CAAA,KAAA,CAAA,SAAA,CAAA,GAAA,EAHA,CAGA;;AACA,YAAA,CAAA,KAAA,MAAA,CAAA,KAAA,CAAA,aAAA,EAAA;AACA,eAAA,OAAA,GAAA,KAAA;AACA;AACA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,OAAA;AACA;AAZA,GAtBA;AAoCA,EAAA,OApCA,qBAoCA;AACA,SAAA,MAAA,CAAA,MAAA,CAAA,qBAAA;AACA,IAAA,OAAA,CAAA,GAAA,CAAA,yCAAA;AACA,IAAA,OAAA,CAAA,GAAA,CAAA,KAAA,MAAA,CAAA,KAAA,CAAA,SAAA,CAAA,aAAA;AACA,SAAA,MAAA,CAAA,QAAA,CAAA,QAAA;AACA;AAzCA,CAAA","sourcesContent":["<template>\r\n    <v-container grid-list-md>\r\n      <v-layout row wrap align-center justify-center fill-height>\r\n        <v-flex xs12 sm8 lg4 md5>\r\n          <v-card class=\"login-card\">\r\n            <v-card-title>\r\n              <span class=\"headline\">Login to Movies</span>\r\n            </v-card-title>\r\n\r\n            <v-spacer/>\r\n\r\n            <v-card-text>\r\n\r\n              <v-layout\r\n                row\r\n                fill-height\r\n                justify-center\r\n                align-center\r\n                v-if=\"loading\"\r\n              >\r\n                <v-progress-circular\r\n                  :size=\"50\"\r\n                  color=\"primary\"\r\n                  indeterminate\r\n                />\r\n              </v-layout>\r\n\r\n\r\n              <v-form v-else ref=\"form\" v-model=\"valid\" lazy-validation>\r\n                <v-container>\r\n\r\n                  <v-text-field\r\n                    v-model=\"credentials.username\"\r\n                    :counter=\"70\"\r\n                    label=\"username\"\r\n                    :rules=\"rules.username\"\r\n                    maxlength=\"70\"\r\n                    required\r\n                  />\r\n\r\n                  <v-text-field\r\n                    type=\"password\"\r\n                    v-model=\"credentials.password\"\r\n                    :counter=\"20\"\r\n                    label=\"password\"\r\n                    :rules=\"rules.password\"\r\n                    maxlength=\"20\"\r\n                    required\r\n                  />\r\n\r\n                </v-container>\r\n                <v-btn class=\"pink white--text\" :disabled=\"!valid\" @click=\"login\">Login</v-btn>\r\n\r\n              </v-form>\r\n\r\n              <v-card class=\"mt-3\" header=\"Form Data Result\">\r\n                <pre class=\"m-0\">{{ credentials }}</pre>\r\n              </v-card>\r\n\r\n            </v-card-text>\r\n          </v-card>\r\n        </v-flex>\r\n      </v-layout>\r\n    </v-container>\r\n</template>\r\n\r\n<script>\r\n\r\n// import swal from 'sweetalert2';\r\n\r\nexport default {\r\n    name: 'Auth',\r\n    data: () => ({\r\n        \r\n        credentials: {\r\n          'username': '',\r\n          'password': ''\r\n        },\r\n        valid:true,\r\n        loading:false,\r\n        rules: {\r\n          username: [\r\n            v => !!v || \"Username is required\",\r\n            v => (v && v.length > 3) || \"A username must be more than 3 characters long\",\r\n            v => /^[a-z0-9_]+$/.test(v) || \"A username can only contain letters and digits\"\r\n          ],\r\n          password: [\r\n            v => !!v || \"Password is required\",\r\n            v => (v && v.length > 7) || \"The password must be longer than 7 characters\"\r\n          ]\r\n        }\r\n    }),\r\n    methods: {\r\n        login() {\r\n          // checking if the input is valid\r\n            if (this.$refs.form.validate()) {\r\n              this.loading = true;\r\n              this.$store.dispatch('obtainToken', this.credentials);    //ejecuta una acci√≥n (vuex para obtener el token)\r\n              console.log(this.$store.state.endpoints.jwt);             //muestra el token por consola\r\n              if(!this.$store.state.authenticated){\r\n                this.loading = false;\r\n              }\r\n            }\r\n            console.log(this.loading)\r\n        }\r\n    },\r\n    created() {\r\n      this.$store.commit('deleteAuthenticated');\r\n      console.log(\"mensaje desde la conosolo del navegador\");\r\n      console.log(this.$store.state.endpoints.authenticated);\r\n      this.$store.dispatch('prueba');\r\n    }\r\n}\r\n</script>"],"sourceRoot":"src/components/pages"}]}